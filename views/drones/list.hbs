<h2>Available drones</h2>
{{#each drones}}
    <div>
        <h3>Name: {{name}}</h3>
        <h4>Propellers: {{propellers}}</h4>
        <h4>Maximum speed: {{maxSpeed}}</h4>
        <a href="/drones/{{_id}}/edit">Edit Drone</a>
        <form action="/drones/{{_id}}/delete" method="post">
            <button type="submit">Delete</button>
        </form>
    </div>
{{/each}}